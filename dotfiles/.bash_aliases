alias gitg='LC_ALL=en_GB gitg'
alias hgrep='history | grep'
alias psa='ps ux -A'
alias e='exa -hla --group-directories-first --git --time-style long-iso'
alias ll='ls -alF'
alias fzf-preview="fzf --preview 'head -100 {}'"
alias ..='cd ..'
alias du_overview='du -h | grep "^[0-9,]*[MG]" | sort -hr | less'
